{
  "compilerOptions": {
    "checkJs": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "lib": ["es6", "dom", "es2017"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "ignoreDeprecations": "6.0",
    "noEmit": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}
